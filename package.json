    "express": "^4.17.1",
    "express-validator": "^6.14.2",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
